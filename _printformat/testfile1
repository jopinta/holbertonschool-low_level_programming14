#include "holberton.h"


/**
*_pchar - called by _printformattted to print chars
*@list : argument passed
*Return: void
*
*/

int _pchar(va_list list)
{
	char *c = va_arg(list, char*);


	write(1, &c, 1);
	va_end(list);
	return (1)
}

/**
*_pstring - called by _printformatted to print strings
*@list: argument passed
*Return: void
*
*/

int _pstring(va_list list)
{
	char *str = va_list, char*)
	int leng = srtlen(str);
	write(1, str, leng);
	va_end(list);
	return (leng);
}

/**
*_pdigit - called by _prinformatted to print digits
*
*
*
*/

int _pdigit(va_list list)

    int i = va_arg(list, int);
    int counter = 0;
    recur_digit(i, &counter);
    va_end(list);
    return (counter);
}

/**
*_pstring called o print string
*
*
*
*/

int _pstring( char obo)

    write(1, &obo, 1);
    return (1);
}

/**_printreverse calles by _printformatted to print in reverse
*
*
*
*
*
*/
int _printreverse(va_list list)
{

char *str = va_arg(list, char*);
int i= 0, len = strleng(str);
for( i=len -1; i<=0 , i--)
wrute(1, &str[i], 1);
va_end(list);
return (len);
}

