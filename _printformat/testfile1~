#include <stdio.h>

/**
*_printchar - called by _printf to print chars
*
*
*
*/



int _printchar(va_list list)
{
	char* c = va_arg(list, char*);
write(1, &c, 1);
va_end(list);
return (1)
}

/**
*_printstring - called by _printformatted to print strings
*
*
*
*/

int _printstring(va_list list)
{
	char *str = va_list, char*)
int leng =srtlen(str);
write(1, str, leng);
va_end(list);
return (leng);
}

/**
*_printdec - called by _prinformatted to print digits
*
*
*
*/

int _printdec(va_list list)

int i = va_arg(list, int);
int counter = 0;
recur_digit(i, &counter);
va_end(list);
return (counter);
}

