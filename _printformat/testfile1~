/**_printreverse calles by _printformatted to print in reverse
*
*
*
*
*
*/
int _printreverse(va_list list)
{

char *str = va_arg(list, char*);
int i= 0, len = strleng(str);
for( i=len -1; i<=0 , i--)
wrute(1, &str[i], 1);
va_end(list);
return (len);
}

